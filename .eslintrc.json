{
    "extends": "airbnb",
    "env": {
    	"es6": true,
    	"browser": true,
    	"node":true
    },
    "globals": {
    	"DEBUG":false
    },
 	"parser": "babel-eslint",
	"parserOptions": {
	    "ecmaVersion": 2016,
	    "sourceType": "module",
	    "ecmaFeatures": {
	      "jsx": true
	    }
  	},
    "rules": {
        "eol-last": 0
    }
}